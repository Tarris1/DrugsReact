var list_of_drugs = [];
for (var i = 1; i < Object.keys(data['drugs']).length; i++) {
	var z = i.toString()
	if (z in data['drugs']){//drugs){
		//console.log(i)
		var dict = {
		"id":z,
		"brand":data['drugs'][z]['name'],
		"category":data['drugs'][z]['category'],
		"firm":data['drugs'][z]['misc'],
		"indications":data['drugs'][z]['disease']
	};
	list_of_drugs.push(dict);
	}
	
	
}